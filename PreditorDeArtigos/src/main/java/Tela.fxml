<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="510.0" prefWidth="630.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projetoIA.controllers.TelaController">
   <children>
      <TabPane layoutX="14.0" layoutY="56.0" prefHeight="381.0" prefWidth="606.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Classificador">
               <content>
                  <Pane prefHeight="351.0" prefWidth="606.0">
                     <children>
                        <TextArea fx:id="txtArtigoEmTexto" layoutX="14.0" layoutY="57.0" prefHeight="209.0" prefWidth="578.0" promptText="DIGITAR TEXTO AQUI" />
                        <Button fx:id="btnClassificar" layoutX="14.0" layoutY="278.0" mnemonicParsing="false" onAction="#classificarArtigo" prefHeight="60.0" prefWidth="578.0" text="CLASSIFICAR" />
                        <TextField fx:id="txtUrlArtigo" layoutX="14.0" layoutY="14.0" prefHeight="34.0" prefWidth="578.0" promptText="DIGITAR URL AQUI" />
                     </children>
                  </Pane>
               </content>
            </Tab>
        </tabs>
      </TabPane>
      <Label alignment="CENTER" layoutX="12.0" layoutY="14.0" prefHeight="34.0" prefWidth="606.0" text="CLASSIFICAR USANDO REDE NEURAL" textFill="#031103" textOverrun="CENTER_ELLIPSIS">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <TextField fx:id="txtQualClasse" alignment="CENTER" editable="false" layoutX="172.0" layoutY="437.0" prefHeight="54.0" prefWidth="291.0" text="Qual a classe?? :)">
         <font>
            <Font size="29.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
